{
    "name": "@metapages/metapage",
    "public": true,
    "version": "1.7.7",
    "description": "Connect web pages together",
    "repository": "https://github.com/metapages/metapage",
    "homepage": "https://metapage.io/",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
