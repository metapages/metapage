        "fast-json-stable-stringify": "^2.1.0",
        "fetch-retry": "^6.0.0",
        "mutative": "^1.0.11",
        "object-hash": "^3.0.0",
        "picomatch-browser": "^2.2.6"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/object-hash": "^2.2.1",
        "rollup-plugin-typescript-paths": "^1.4.0",
        "typescript": "^5.3.3",
        "vite": "^4.4.11"
    },
    "keywords": [
        "iframe",
        "metaframe",
        "metapage",
